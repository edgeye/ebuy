<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="net.gupt.ebuy.pojo.Admin" table="admin">
		<id name="id" type="java.lang.Integer" column="a_id">
			<!-- generator用来指明主键的生成方式  Mysql 数据库一般使用 identity-->
			<generator class="identity"></generator>
		</id>
		<property name="name" type="java.lang.String" column="a_name"></property>
		<property name="pass" type="java.lang.String" column="a_pass"></property>
		<property name="header" type="java.lang.String" column="a_header"></property>
		<property name="phone" type="java.lang.String" column="a_phone"></property>
		<property name="email" type="java.lang.String" column="a_email"></property>
	</class>
</hibernate-mapping>